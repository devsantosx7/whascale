{
   "action": {
      "default_icon": "label/icons/plugin/icon.png"
   },
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [ {
      "css": [ "label/css/whascale.css", "content/assets/css/style.css", "content.css" ],
      "js": [ "content/index.js" ],
      "matches": [ "https://web.whatsapp.com/*" ]
   } ],
   "description": "WhaScale é um CRM para WhatsApp Web. Organize seus chats e otimize seus negócios no WhatsApp Web",
   "externally_connectable": {
      "matches": [ "https://app.wascript.com.br/*", "https://dev.watools.com.br/*" ]
   },
   "icons": {
      "128": "label/icons/plugin/icon.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "WhaScale - Um passo à frente do seu concorrente",
   "permissions": [ "unlimitedStorage", "storage", "alarms", "tabs" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "7.4.3.14",
   "web_accessible_resources": [ {
      "matches": [ "https://web.whatsapp.com/*" ],
      "resources": [ "*.js", "*.css", "*.png", "*.mp3", ".ttf", "label/*.json", "label/*.png" ]
   } ]
}
